<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LogInController">
   <children>
      <Label layoutX="22.0" layoutY="25.0" text="Appointment Management System">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label fx:id="usernameLabel" layoutX="172.0" layoutY="110.0" text="Username" />
      <Label fx:id="passwordLabel" layoutX="172.0" layoutY="149.0" text="Password" />
      <Label fx:id="timeZoneLabel" layoutX="171.0" layoutY="189.0" text="Time Zone" />
      <Label fx:id="userTimeZoneLabel" layoutX="264.0" layoutY="189.0" text="Country/Division" />
      <TextField fx:id="usernameText" layoutX="264.0" layoutY="106.0" prefHeight="25.0" prefWidth="185.0" />
      <Button fx:id="logInButton" layoutX="171.0" layoutY="268.0" mnemonicParsing="false" onAction="#onLogInButton" text="Log In" />
      <Button fx:id="resetButton" layoutX="278.0" layoutY="268.0" mnemonicParsing="false" onAction="#onResetButton" text="Reset" />
      <Button fx:id="cancelButton" layoutX="381.0" layoutY="268.0" mnemonicParsing="false" onAction="#onCancelButton" text="Cancel" />
      <PasswordField fx:id="passwordText" layoutX="264.0" layoutY="145.0" prefHeight="25.0" prefWidth="185.0" />
   </children>
</AnchorPane>
