<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ModifyCustomerController">
   <children>
      <Label layoutX="35.0" layoutY="35.0" text="Modify Customer">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="53.0" layoutY="89.0" text="Customer ID" />
      <Label layoutX="53.0" layoutY="121.0" text="Customer Name" />
      <Label layoutX="53.0" layoutY="152.0" text="Address" />
      <Label layoutX="53.0" layoutY="183.0" text="Postal Code" />
      <Label layoutX="53.0" layoutY="215.0" text="Phone Number" />
      <Label layoutX="53.0" layoutY="246.0" text="Country" />
      <Label layoutX="339.0" layoutY="246.0" text="Division" />
      <TextField fx:id="customerIdTextField" layoutX="159.0" layoutY="85.0" />
      <TextField fx:id="customerNameTextField" layoutX="159.0" layoutY="117.0" />
      <TextField fx:id="addressTextField" layoutX="159.0" layoutY="148.0" />
      <TextField fx:id="postalCodeTextField" layoutX="159.0" layoutY="179.0" />
      <TextField fx:id="phoneNumberTextField" layoutX="159.0" layoutY="211.0" />
      <ComboBox fx:id="countryComboBox" layoutX="159.0" layoutY="242.0" onAction="#onCountryComboBox" prefWidth="150.0" />
      <ComboBox fx:id="divisionComboBox" layoutX="409.0" layoutY="242.0" onAction="#onDivisionComboBox" prefWidth="150.0" />
      <Button fx:id="saveButton" layoutX="198.0" layoutY="318.0" mnemonicParsing="false" onAction="#onSaveButton" prefHeight="25.0" prefWidth="80.0" text="Save" />
      <Button fx:id="cancelButton" layoutX="329.0" layoutY="318.0" mnemonicParsing="false" onAction="#onCancelButton" prefHeight="25.0" prefWidth="80.0" text="Cancel" />
   </children>
</AnchorPane>
